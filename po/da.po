#: nautilus/GNOME_SuperUser_Component.server.in.in.h:1
msgid "GNOME SuperUser integration"
msgstr "GNOME-administratorintegration"

#: nautilus/GNOME_SuperUser_Component.server.in.in.h:2
msgid "Open as superuser (root)..."
msgstr "Åbn som administrator ..."

#: src/gnomesu-auth-dialog.c:110
msgid "Password needed"
msgstr "Adgangskode behøves"

#: src/gnomesu-auth-dialog.c:163
msgid "Command:"
msgstr "Kommando:"

#: src/gnomesu-auth-dialog.c:363
msgid "Administrator (root) privilege is required."
msgstr "Administratorprivilegier kræves."

#: src/gnomesu-auth-dialog.c:364
msgid "Please enter the root password to continue."
msgstr "Angiv venligst adgangskoden for at fortsætte."

#: src/gnomesu-auth-dialog.c:413
msgid "_Password:"
msgstr "_Adgangskode:"

#: src/gnomesu-auth-dialog.c:438 src/services/sudo.c:151
msgid "Please wait, verifying password..."
msgstr "Vent venligst, efterprøver adgangskoden..."

#: src/gnomesu-auth-dialog.c:446 src/services/sudo.c:135
msgid "Incorrect password, please try again."
msgstr "Ugyldig adgangskode, forsøg igen."

#: src/gnomesu-auth-dialog.c:453
msgid "Incorrect password, please try again. You have one more chance."
msgstr "Ugyldig adgangskode, forsøg igen. Du har én chance til."

#: src/libgnomesu.c:170
msgid "No services for libgnomesu are available.\n"
msgstr "Ingen tilgængelige tjenester for libgnomesu.\n"

#: src/services/pam.c:192 src/services/su.c:185
#, c-format
msgid "Please enter %s's password and click Run to continue."
msgstr "Angiv adgangskoden for %s og klik på Kør for at fortsætte."

#: src/services/pam.c:194 src/services/su.c:187
msgid "The requested action needs further authentication."
msgstr "Den forespurgte handling kræver yderligere godkendelse."

#: src/services/pam.c:200 src/services/su.c:193
#, c-format
msgid "%s's _password:"
msgstr "_Adgangskode for %s:"

#: src/services/pam.c:219 src/services/su.c:212
#, c-format
msgid "User '%s' doesn't exist."
msgstr "Brugeren \"%s\" findes ikke."

#: src/services/pam.c:224 src/services/su.c:216
msgid "An unknown error occured while authenticating."
msgstr "En ukendt fejl opstod under godkendelsen."

#: src/services/pam.c:228 src/services/su.c:220
msgid "You do not have permission to authenticate."
msgstr "Du har ikke rettigheder til at godkende."

#: src/services/pam.c:232
msgid "Unable to initialize the PAM authentication system."
msgstr "Kunne ikke initialisere PAM-godkendelsessystemet."

#: src/services/sudo.c:320
msgid ""
"You do not have permission gain superuser (root) privileges. This incident "
"will be reported."
msgstr ""
"Du har ikke ret til at få administratorrettigheder. Denne hændelse vil blive "
"rapporteret."

