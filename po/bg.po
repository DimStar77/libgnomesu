#: nautilus/GNOME_SuperUser_Component.server.in.in.h:1
msgid "GNOME SuperUser integration"
msgstr "Интеграция с администратора за GNOME"

#: nautilus/GNOME_SuperUser_Component.server.in.in.h:2
msgid "Open as superuser (root)..."
msgstr "Отваряне като администратор (root)..."

#: src/libgnomesu.c:170
msgid "No services for libgnomesu are available.\n"
msgstr "Няма налични услуги за libgnomesu.\n"

#: src/gnomesu-auth-dialog.c:108
msgid "Password needed"
msgstr "Необходима е парола"

#: src/gnomesu-auth-dialog.c:161
msgid "Command:"
msgstr "Команда:"

#: src/gnomesu-auth-dialog.c:359
msgid ""
"<b>Administrator (root) privilege is required.</b>\n"
"Please enter the root password to continue."
msgstr ""
"<b>Необходими са права на администратор (root).</b>\n"
"Въведете паролата на администратора, за да продължите."

#: src/gnomesu-auth-dialog.c:405
msgid "_Password:"
msgstr "_Парола:"

#: src/gnomesu-auth-dialog.c:430 src/services/sudo.c:151
msgid "Please wait, verifying password..."
msgstr "Изчакайте, паролата се проверява..."

#: src/gnomesu-auth-dialog.c:438 src/services/sudo.c:135
msgid "Incorrect password, please try again."
msgstr "Невярна парола, опитайте отново."

#: src/gnomesu-auth-dialog.c:445
msgid "Incorrect password, please try again. You have one more chance."
msgstr "Невярна парола, опитайте отново. Това е последният ви опит."

#: src/services/pam.c:192 src/services/su.c:185
#, c-format
msgid "Please enter %s's password and click Run to continue."
msgstr ""
"Въведете паролата на \"%s\" и натиснете \"Стартиране\", за да продължите."

#: src/services/pam.c:194 src/services/su.c:187
msgid "The requested action needs further authentication."
msgstr "Необходима е допълнителна идентификация за заявеното действие."

#: src/services/pam.c:200 src/services/su.c:193
#, c-format
msgid "%s's _password:"
msgstr "Парола на _%s"

#: src/services/pam.c:219 src/services/su.c:212
#, c-format
msgid "User '%s' doesn't exist."
msgstr "Потребителят \"%s\" не съществува."

#: src/services/pam.c:224 src/services/su.c:216
msgid "An unknown error occured while authenticating."
msgstr "Възникна непозната грешка при идентификацията."

#: src/services/pam.c:228 src/services/su.c:220
msgid "You do not have permission to authenticate."
msgstr "Нямате права за идентификация."

#: src/services/pam.c:232
msgid "Unable to initialize the PAM authentication system."
msgstr "Неуспех при стартиране на системата за идентификация PAM."

#: src/services/sudo.c:320
msgid ""
"You do not have permission gain superuser (root) privileges. This incident "
"will be reported."
msgstr ""
"Не ви е разрешено да получите административни права. Този случай ще бъде "
"докладван."

