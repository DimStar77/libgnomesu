#: nautilus/GNOME_SuperUser_Component.server.in.in.h:1
msgid "GNOME SuperUser integration"
msgstr "Tích hợp GNOME SuperUser"

#: nautilus/GNOME_SuperUser_Component.server.in.in.h:2
msgid "Open as superuser (root)..."
msgstr "Mở bằng superuser (root)..."

#: src/libgnomesu.c:170
msgid "No services for libgnomesu are available.\n"
msgstr "Hiện không có dịch vụ nào cho libgnomesu.\n"

#: src/gnomesu-auth-dialog.c:108
msgid "Password needed"
msgstr "Yêu cầu mật khẩu"

#: src/gnomesu-auth-dialog.c:161
msgid "Command:"
msgstr "Lệnh:"

#: src/gnomesu-auth-dialog.c:359
msgid ""
"<b>Administrator (root) privilege is required.</b>\n"
"Please enter the root password to continue."
msgstr ""
"<b>Yêu cầu phải có đặc quyền của nhà quản trị (root).</b>\n"
"Hãy nhập mật khẩu root để tiếp tục."

#: src/gnomesu-auth-dialog.c:405
msgid "_Password:"
msgstr "_Mật khẩu:"

#: src/gnomesu-auth-dialog.c:430
#: src/services/sudo.c:151
msgid "Please wait, verifying password..."
msgstr "Hãy chờ, đang kiểm tra mật khẩu..."

#: src/gnomesu-auth-dialog.c:438
#: src/services/sudo.c:135
msgid "Incorrect password, please try again."
msgstr "Sai mật khẩu, hãy nhập lại."

#: src/gnomesu-auth-dialog.c:445
msgid "Incorrect password, please try again. You have one more chance."
msgstr "Sai mật khẩu, hãy nhập lại. Bạn có một lần nhập nữa."

#: src/services/pam.c:192
#: src/services/su.c:185
#, c-format
msgid "Please enter %s's password and click Run to continue."
msgstr "Hãy nhập mật khẩu của %s và nhấn Chạy để tiếp tục."

#: src/services/pam.c:194
#: src/services/su.c:187
msgid "The requested action needs further authentication."
msgstr "Hành động đó cần phải xác thực thêm."

#: src/services/pam.c:200
#: src/services/su.c:193
#, c-format
msgid "%s's _password:"
msgstr "Mật khẩu của %s:"

#: src/services/pam.c:219
#: src/services/su.c:212
#, c-format
msgid "User '%s' doesn't exist."
msgstr "Người dùng '%s' không tồn tại."

#: src/services/pam.c:224
#: src/services/su.c:216
msgid "An unknown error occured while authenticating."
msgstr "Xảy ra lỗi không xác định khi xác thực."

#: src/services/pam.c:228
#: src/services/su.c:220
msgid "You do not have permission to authenticate."
msgstr "Bạn không có quyền hạn để xác thực."

#: src/services/pam.c:232
msgid "Unable to initialize the PAM authentication system."
msgstr "Không thể khởi tạo hệ thống xác thực PAM."

#: src/services/sudo.c:320
msgid "You do not have permission gain superuser (root) privileges. This incident will be reported."
msgstr "Bạn không có quyền hạn để có đặc quyền của superuser (root). Việc này sẽ được báo cáo."

