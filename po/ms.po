#: nautilus/GNOME_SuperUser_Component.server.in.in.h:1
msgid "GNOME SuperUser integration"
msgstr "Integrasi PenggunaSuper GNOME"

#: nautilus/GNOME_SuperUser_Component.server.in.in.h:2
msgid "Open as superuser (root)..."
msgstr "Buka sebagai PenggunaSuper (root)..."

#: src/pam.c:99
#: src/su.c:97
#, c-format
msgid "%s's _password:"
msgstr "_katalaluan %s:"

#: src/pam.c:103
#: src/su.c:101
#, c-format
msgid "Please enter %s's password and click Run to continue."
msgstr "Sila masukkan katalaluan %s dan klik Laksana untuk meneruskan."

#: src/pam.c:105
#: src/su.c:103
msgid "The requested action needs further authentication."
msgstr "Aksi dipinta memerlukan authentikasi lanjutan."

#: src/pam.c:164
#: src/su.c:162
msgid "Incorrect password, please try again. You have one more chance."
msgstr "Katalaluan salah, sila cuba lagi. Anda mempunya satu lagi peluang."

#
#: src/pam.c:170
#: src/su.c:168
#: src/sudo.c:135
msgid "Incorrect password, please try again."
msgstr "Katalaluan tidak sepadan. Sila cuba lagi."

#: src/pam.c:186
#: src/su.c:185
#: src/sudo.c:151
msgid "Please wait, verifying password..."
msgstr "Sila tunggu, menentusahkan katalaluan..."

#: src/pam.c:352
#: src/su.c:348
#, c-format
msgid "User '%s' doesn't exist."
msgstr "pengguna '%s' tidak wujud."

#: src/pam.c:357
#: src/su.c:353
msgid "An unknown error occured while authenticating."
msgstr "Ralat tidak diketahui berlaku bila cuba mengauthentikasi"

#: src/pam.c:361
#: src/su.c:357
msgid "You do not have permission to authenticate."
msgstr "Anda tidak mempunyai keizinan untuk mengauthentikasi."

#: src/sudo.c:313
msgid "You do not have permission gain superuser (root) privileges. This incident will be reported."
msgstr "Anda tak mempunyai keizinan untuk memperolehi privileh penggunasuper (root). Insiden ini akan dilaporkan."

