#: nautilus/GNOME_SuperUser_Component.server.in.in.h:1
msgid "GNOME SuperUser integration"
msgstr "GNOME SuperUser integratie"

#: nautilus/GNOME_SuperUser_Component.server.in.in.h:2
msgid "Open as superuser (root)..."
msgstr "Openen als supergebruiker (root)..."

#: src/libgnomesu.c:169
msgid "No services for libgnomesu are available.\n"
msgstr "Er zijn geen diensten voor libgnomesu beschikbaar.\n"

#: src/pam.c:100 src/su.c:98
#, c-format
msgid "%s's _password:"
msgstr "Wachtwoord van %s:"

#: src/pam.c:104 src/su.c:102
#, c-format
msgid "Please enter %s's password and click Run to continue."
msgstr "Voer het wachtwoord van %s in en klik op Uitvoeren om door te gaan."

#: src/pam.c:106 src/su.c:104
msgid "The requested action needs further authentication."
msgstr "De vereiste actie vereist verdere authentificatie."

#: src/pam.c:165 src/su.c:163
msgid "Incorrect password, please try again. You have one more chance."
msgstr "Wachtwoord onjuist, probeer het nogmaals. U heeft nog één kans."

#: src/pam.c:171 src/su.c:169 src/sudo.c:135
msgid "Incorrect password, please try again."
msgstr "Wachtwoord onjuist; probeer het nogmaals."

#: src/pam.c:187 src/su.c:186 src/sudo.c:151
msgid "Please wait, verifying password..."
msgstr "Het wachtwoord wordt gecontroleerd, even geduld a.u.b...."

#: src/pam.c:361 src/su.c:354
#, c-format
msgid "User '%s' doesn't exist."
msgstr "De gebruiker '%s' bestaat niet."

#: src/pam.c:366 src/su.c:359
msgid "An unknown error occured while authenticating."
msgstr "Een onbekende fout is opgetreden tijdens authentificatie."

#: src/pam.c:370 src/su.c:363
msgid "You do not have permission to authenticate."
msgstr "U heeft geen toegangsrechten om te authentificeren."

#: src/pam.c:374
msgid "Unable to initialize PAM authentication system."
msgstr "Niet in staat het PAM aanmeldingssysteem te initialiseren."

#: src/sudo.c:320
msgid ""
"You do not have permission gain superuser (root) privileges. This incident "
"will be reported."
msgstr ""
"U heeft geen toegangsrechten om supergebruiker-rechten te verkrijgen.\n"
"Dit incident zal worden gerapporteerd."

