#: src/pam.c:99 src/su.c:97
#, c-format
msgid "%s's _password:"
msgstr "%s's _wachtwoord:"

#: src/pam.c:104 src/su.c:102
#, c-format
msgid ""
"<span weight=\"bold\">The requested action needs further authentication.</"
"span>\n"
"Please enter %s's password and click Run to continue."
msgstr ""
"<span weight=\"bold\">De vereiste actie vereist verdere verificatie.</span>\n"
"Voer %s's wachtwoord in en klik op Uitvoeren om door te gaan."

#: src/pam.c:160 src/su.c:158
msgid ""
"<span style=\"italic\" weight=\"bold\">Incorrect password, please try again. "
"You have one more chance.</span>"
msgstr ""
"<span style=\"italic\" weight=\"bold\">Onjuiste wachtwoord, probeer het "
"nogmaals. U heeft nog één kans.</span>"

#: src/pam.c:164 src/su.c:162 src/sudo.c:133
msgid ""
"<span style=\"italic\" weight=\"bold\">Incorrect password, please try again."
"</span>"
msgstr ""
"<span style=\"italic\" weight=\"bold\">Wachtwoord onjuist; probeer het "
"nogmaals.</span>"

#: src/pam.c:177 src/su.c:175 src/sudo.c:145 src/ui/su-strings.h:14
#: src/ui/sudo-strings.h:14
msgid ""
"<span style=\"italic\" weight=\"bold\">Please wait, verifying password...</"
"span>"
msgstr ""
"<span style=\"italic\" weight=\"bold\">Het wachtwoord wordt bevestigd, even "
"geduld a.u.b....</span>"

#: src/pam.c:340 src/su.c:335
#, c-format
msgid "User '%s' doesn't exist."
msgstr "De gebruiker '%s' bestaat niet."

#: src/pam.c:345 src/su.c:340
msgid "An unknown error occured while authenticating."
msgstr "En onbekende fout is opgetreden tijdens verificatie."

#: src/pam.c:349 src/su.c:344
msgid "You do not have permission to authenticate."
msgstr "U heeft geen permissie om zich te verifiëren."

#: src/sudo.c:305
msgid ""
"You do not have permission gain superuser (root) privileges. This incident "
"will be reported."
msgstr ""
"U heeft geen permissie om systeembeheerder-rechten te verkrijgen.\n"
"Dit incident zal worden gerapporteerd."

